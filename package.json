{
  "name": "abrahamcalsin",
  "version": "v3.0.0",
  "description": "my personal website",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "pnpm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "format": "pnpm run prettier -- --write",
    "check:format": "pnpm run prettier -- --list-different",
    "clean": "rimraf -rf .next .contentlayer",
    "contentlayer:build": "./node_modules/.bin/contentlayer build",
    "prepare": "husky install",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook -s public -o storybook-build"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@notionhq/client": "^2.2.0",
    "@types/react": "^18.0.14",
    "clsx": "1.1.1",
    "contentlayer": "^0.2.5",
    "dayjs": "1.10.7",
    "framer-motion": "^6.3.16",
    "gray-matter": "4.0.3",
    "just-random": "^3.1.1",
    "next": "^12.0.10",
    "next-contentlayer": "^0.2.5",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "reading-time": "^1.5.0",
    "rehype": "^12.0.1",
